{
  "name": "localpro",
  "version": "0.1.0",
  "description": "LocalPro - A comprehensive platform connecting service providers with clients in the Philippines",
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "firebase",
    "payment",
    "service-provider",
    "philippines"
  ],
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 9006",
    "dev:turbo": "next dev --turbopack -p 9006",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "email": "email dev",
    "validate-payments": "tsx src/scripts/validate-payment-system.ts",
    "setup-dev": "tsx src/scripts/setup-dev-environment.ts",
    "seed:learning-hub": "tsx src/scripts/seed-learning-hub.ts",
    "seed:learning-hub:mock": "tsx src/scripts/seed-learning-hub-mock.ts",
    "test-paypal": "tsx src/scripts/test-paypal-integration.ts",
    "test-maya": "tsx src/scripts/test-maya-integration.ts",
    "setup-payment-config": "tsx src/scripts/setup-payment-config.ts",
    "setup-payment-gateways": "tsx src/scripts/setup-payment-gateways.ts",
    "check-payment-config": "tsx src/scripts/check-payment-config.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf .next out dist",
    "analyze": "npm run build && npx @next/bundle-analyzer",
    "audit:fix": "npm audit fix",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "firebase:deploy": "firebase deploy",
    "firebase:emulators": "firebase emulators:start",
    "n8n:start": "docker-compose -f docker-compose.n8n.yml up -d",
    "n8n:stop": "docker-compose -f docker-compose.n8n.yml down",
    "n8n:logs": "docker-compose -f docker-compose.n8n.yml logs -f",
    "n8n:backup": "docker exec localpro-n8n n8n export:workflow --all --output=/home/node/.n8n/backup"
  },
  "dependencies": {
    "@genkit-ai/googleai": "^1.13.0",
    "@genkit-ai/next": "^1.13.0",
    "@hookform/resolvers": "^3.9.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-jaeger": "^2.1.0",
    "@paypal/paypal-js": "^9.0.1",
    "@paypal/react-paypal-js": "^8.9.2",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-email/components": "^0.5.5",
    "@tanstack/react-table": "^8.21.3",
    "@testing-library/dom": "^10.4.1",
    "@types/redis": "^4.0.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "embla-carousel": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "firebase": "^11.9.1",
    "firebase-admin": "^13.5.0",
    "genkit": "^1.13.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.475.0",
    "next": "^15.5.4",
    "next-intl": "^4.3.5",
    "next-themes": "^0.3.0",
    "qrcode.react": "^3.1.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-email": "^4.2.12",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "redis": "^5.8.3",
    "resend": "^3.5.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "twilio": "^5.10.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@next/bundle-analyzer": "^15.5.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/qrcode.react": "^1.0.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "critters": "^0.0.23",
    "eslint": "^9.37.0",
    "eslint-config-next": "15.5.4",
    "genkit-cli": "^1.13.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.4",
    "typescript": "^5"
  }
}
